<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE beans PUBLIC '-//SPRING//DTD BEAN//EN' 'http://www.springframework.org/dtd/spring-beans.dtd'>
<beans>
	<bean id="alvex-advanced-workflows-messages-deployer" class="org.springframework.extensions.surf.util.ResourceBundleBootstrapComponent" depends-on="">
		<property name="resourceBundles">
			 <list>
				<value>alfresco.web-extension.messages.alvex-advanced-workflows</value>
				<value>alfresco.web-extension.messages.alvex-my-workflows-dashlet</value>
			</list>
		</property>
	</bean>
	<bean id="alvex-advanced-workflows" class="com.alvexcore.share.AdvancedWorkflowsExtension" parent="alvex-extension"/>
	<bean id="webscript.com.alvexcore.components.task-form.form.get" class="com.alvexcore.share.web.scripts.forms.TaskFormUIGet" parent="webscript">
		<property name="configService" ref="web.config" />
	</bean>
</beans>
